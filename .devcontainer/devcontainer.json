{
    "name": "coffeenote",
    "build": {
        "dockerfile": "Dockerfile",
        "context": ".."
    },
    "features": {
        "docker-from-docker": {
            "version": "latest",
            "moby": true
        },
        "docker-from-docker-compose": {
            "version": "latest",
            "moby": true
        },
        "git": {
            "version": "latest",
            "ppa": false
        }
    },
    "extensions": [
        "bungcip.better-toml",
        "zxh404.vscode-proto3",
        "matklad.rust-analyzer"
    ],
    "remoteEnv": {
        "HOST_PROJECT_ROOT": "${localWorkspaceFolder}"
    }
}
